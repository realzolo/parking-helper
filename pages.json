{

	"pages": [ //pages数组中第一项表示应用启动页，参考：https://uniapp.dcloud.io/collocation/pages
		{
			"path": "pages/index/index",
			"style": {
				"navigationBarTitleText": "首页",
				"enablePullDownRefresh": true
			}
		}, {
			"path": "pages/stall/stall",
			"style": {
				"navigationBarTitleText": "车位",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/order/order",
			"style": {
				"navigationBarTitleText": "订单",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "pages/profile/profile",
			"style": {
				"navigationBarTitleText": "我的",
				"enablePullDownRefresh": false
			}
		}
	],
	"subPackages": [{
		"root": "subpages",
		"pages": [{
			"path": "wallet",
			"style": {
				"navigationBarTitleText": "我的钱包",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "coupon",
			"style": {
				"navigationBarTitleText": "我的优惠券",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "carlist",
			"style": {
				"navigationBarTitleText": "车辆管理",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "caredit",
			"style": {
				"navigationBarTitleText": "编辑车辆信息",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "carport",
			"style": {
				"navigationBarTitleText": "车库列表",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "carport-detail",
			"style": {
				"navigationBarTitleText": "车位详情",
				"enablePullDownRefresh": false
			}
		}, {
			"path": "result",
			"style": {
				"navigationBarTitleText": "订单结果",
				"enablePullDownRefresh": false
			}
		}]
	}],
	"globalStyle": {
		"navigationBarTextStyle": "black",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F8F8F8",
		"usingComponents": {
			"van-button": "/wxcomponents/vant/dist/button/index",
			"van-image": "/wxcomponents/vant/dist/image/index",
			"van-cell": "/wxcomponents/vant/dist/cell/index",
			"van-cell-group": "/wxcomponents/vant/dist/cell-group/index",
			"van-icon": "/wxcomponents/vant/dist/icon/index",
			"van-tab": "/wxcomponents/vant/dist/tab/index",
			"van-tabs": "/wxcomponents/vant/dist/tabs/index",
			"van-card": "/wxcomponents/vant/dist/card/index",
			"van-field": "/wxcomponents/vant/dist/field/index",
			"van-notify": "/wxcomponents/vant/dist/notify/index",
			"van-share-sheet": "/wxcomponents/vant/dist/share-sheet/index",
			"van-submit-bar": "/wxcomponents/vant/dist/submit-bar/index",
			"van-dialog": "/wxcomponents/vant/dist/dialog/index"
		}
	},
	"tabBar": {
		"color": "#7A7E83",
		"selectedColor": "#3cc51f",
		"borderStyle": "black",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/image/home.png",
			"selectedIconPath": "static/image/home-active.png",
			"text": "首页"
		}, {
			"pagePath": "pages/stall/stall",
			"iconPath": "static/image/stall.png",
			"selectedIconPath": "static/image/stall-active.png",
			"text": "车位"
		}, {
			"pagePath": "pages/order/order",
			"iconPath": "static/image/order.png",
			"selectedIconPath": "static/image/order-active.png",
			"text": "订单"
		}, {
			"pagePath": "pages/profile/profile",
			"iconPath": "static/image/profile.png",
			"selectedIconPath": "static/image/profile-active.png",
			"text": "我的"
